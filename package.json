{
	"name": "tantalus",
	"version": "1.0.0",
	"description": "Tantalus must 'speak' with MongoDB and export Developer query to JSON or CSV",
	"private": true,
	"engines": {
		"node": "^8.9.4"
	},
	"scripts": {
		"build": "npm run del && npm run compile && npm run copy && npm run build:parse",
		"build:parse": "babel develop/parse-server/src/ -d server/parse-server/node_modules/lib/ --copy-files",
		"compile": "tsc && npm run compile:babel",
		"compile:babel": "babel develop/parse-server/src/ -d develop/parse-server/node_modules/lib/ --copy-files",
		"copy": "npm run copy:configurations && npm run copy:parse-server-json",
		"copy:configurations": "cp -R develop/config/ server/config/ || true",
		"copy:parse-server-json": "cp develop/shared/helpers/parse-server/parse-server.json server/shared/helpers/parse-server/parse-server.json",
		"debug": "chmod +x run-debug && ./run-debug",
		"dev": "nodemon server/server.js",
		"del:agenda": "rm -rf agenda/ && mkdir agenda",
		"del": "npm run del:agenda && rm -rf server/",
		"start:agenda": "node server/agenda/worker.js",
		"start:api": "node server/api/server.js",
		"start:cluster": "sl-run server/server.js",
		"start:mongodb": "node server/agenda/mongodb.js",
		"start": "node server/index.js",
		"test:agenda": "npm run build && mocha './server/agenda/test/**/*.spec.js' --recursive",
		"test:api": "npm run build && mocha './server/api/test/**/*.spec.js' --recursive",
		"test:parse": "cross-env MONGODB_VERSION=${MONGODB_VERSION:=3.2.6} MONGODB_STORAGE_ENGINE=mmapv1 TESTING=1 jasmine",
		"test:shared": "npm run build && mocha './server/shared/test/**/*.spec.js' --recursive",
		"test:mocha": "mocha './server/**/test/**/*.spec.js' --recursive",
		"test": "npm run build && npm run test:mocha",
		"report": "nyc report",
		"postinstall": "npm run build"
	},
	"nodemonConfig": {
		"env": {
			"NODE_HEAPDUMP_OPTIONS": "nosignal"
		}
	},
	"dependencies": {
		"agenda": "^2.0.2",
		"appmetrics-dash": "^4.0.0",
		"appmetrics-prometheus": "^2.0.0",
		"appmetrics-zipkin": "^1.0.4",
		"archiver": "^3.0.0",
		"aws-sdk": "^2.350.0",
		"body-parser": "^1.18.3",
		"browser-headers": "^0.4.0",
		"express": "^4.16.4",
		"grpc-web-client": "^0.6.3",
		"lodash": "^4.17.11",
		"log4js": "^3.0.6",
		"mongo": "^0.1.0",
		"mongodb": "^3.1.9",
		"mongoose": "^5.3.10",
		"multer": "^1.4.1",
		"p-iteration": "^1.1.7",
		"parse": "^2.1.0",
		"protos": "git+https://0b053dd6865f9321e6e2014449327f171bc2d847@github.com/cloudstrap/protos.git",
		"reflect-metadata": "^0.1.12",
		"routing-controllers": "^0.7.7",
		"strong-supervisor": "^6.2.0",
		"tus-js-client": "1.5.1",
		"tus-node-server": "^0.3.2",
		"typedi": "^0.8.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.1.5",
		"@babel/core": "^7.1.5",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0",
		"@babel/plugin-transform-flow-strip-types": "^7.0.0",
		"@babel/preset-env": "^7.1.5",
		"@types/agenda": "^2.0.3",
		"@types/body-parser": "^1.17.0",
		"@types/chai": "^4.1.7",
		"@types/chai-as-promised": "^7.1.0",
		"@types/chai-fs": "^2.0.1",
		"@types/chai-http": "^3.0.5",
		"@types/express": "^4.16.0",
		"@types/mocha": "^5.2.5",
		"@types/mongodb": "^3.1.14",
		"@types/mongoose": "^5.3.1",
		"@types/multer": "^1.3.7",
		"@types/node": "^10.12.2",
		"chai": "^4.2.0",
		"chai-as-promised": "^7.1.1",
		"chai-fs": "^2.0.0",
		"chai-http": "^4.2.0",
		"chakram": "^1.5.0",
		"chalk": "^1.1.3",
		"cross-env": "^5.2.0",
		"deep-diff": "^1.0.2",
		"jasmine": "^3.3.0",
		"jasmine-spec-reporter": "^4.2.1",
		"mocha": "^5.2.0",
		"mock-fs": "^4.7.0",
		"mongodb-memory-server": "^2.7.0",
		"nodemon": "^1.18.6",
		"nyc": "^10.3.2",
		"prompt-confirm": "^1.2.0",
		"request": "^2.82.0"
	}
}
